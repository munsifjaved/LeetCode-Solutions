    "import pandas as pd\n",
    "\n",
    "def not_boring_movies(cinema: pd.DataFrame) -> pd.DataFrame:\n",
    "    return cinema[(cinema.id%2 !=0) & (cinema.description!='boring')].sort_values(by='rating',ascending=False)"
