    "import pandas as pd\n",
    "\n",
    "def find_classes(courses: pd.DataFrame) -> pd.DataFrame:\n",
    "    result=courses.groupby('class')['student'].count().reset_index(name='stud_count')\n",
    "    return result[result.stud_count>=5][['class']]"
