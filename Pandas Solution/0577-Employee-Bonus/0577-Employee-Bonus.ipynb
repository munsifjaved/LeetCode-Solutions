    "import pandas as pd\n",
    "\n",
    "def employee_bonus(employee: pd.DataFrame, bonus: pd.DataFrame) -> pd.DataFrame:\n",
    "    merged=employee.merge(bonus, on='empId',how='left')\n",
    "    return merged[(merged.bonus<1000)|(merged.bonus.isna())][['name','bonus']]"
   ]
  }
